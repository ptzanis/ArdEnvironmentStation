  if(componentName.text != \"\")
	{
    writeXmlFile();
    
    generationLog.append(\"XML file created.\");
    generationLog.append(\"--------------------------------------------------------------------------------------------------\");
    
  	if(dynlen(g_dsXmlDescription) > 0)
