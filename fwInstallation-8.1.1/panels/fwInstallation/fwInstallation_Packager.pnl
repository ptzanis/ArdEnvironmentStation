// ------------------------------------------------------------------------------------------------

applySelection(int row, string column, string value)
{ 
  bool bSelected = !this.cellValueRC(row, \"inSelection\");
  
  this.cellValueRC(row, \"inSelection\", bSelected);
  
  setRowBackgroundColor(this.name, row, bSelected);
    
  reloadButton.enabled(TRUE);
}
					
// ------------------------------------------------------------------------------------------------

setRowBackgroundColor(string tableName, int row, bool bSelected)
{
  string columnName;
  shape table = getShape(tableName);

  for(int i = 0; i < table.columnCount; i++)
  {
    columnName = table.columnName(i);
    
    if(bSelected)
    {
      table.cellBackColRC(row, columnName, \"FwInstallationTableSelection\");
    }
    else
    {
      table.cellBackColRC(row, columnName, \"_Window\");
    }
  }
}
				
