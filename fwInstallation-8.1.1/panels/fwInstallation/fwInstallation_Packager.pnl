clearComponentContent()
{
  mappingClear(g_mComponentContent);
  
  g_mComponentContent[KEY_NAME] = \"\";
  g_mComponentContent[KEY_VERSION] = \"1.0.0\";
  g_mComponentContent[KEY_DATE] = formatTime(\"%d/%m/%Y\", getCurrentTime());
