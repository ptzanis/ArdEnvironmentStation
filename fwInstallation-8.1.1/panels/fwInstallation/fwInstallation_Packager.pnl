	loadSelection(KEY_PANEL, g_mComponentContent[KEY_PANEL]);
	loadSelection(KEY_SCRIPT, g_mComponentContent[KEY_SCRIPT]);
	loadSelection(KEY_LIBRARY, g_mComponentContent[KEY_LIBRARY]);
	loadSelection(KEY_OTHER, g_mComponentContent[KEY_OTHER]);
  loadSelection(KEY_BIN, g_mComponentContent[KEY_BIN]);
  loadSelection(KEY_HELP, g_mComponentContent[KEY_HELP]);
  loadSelection(KEY_PICTURE, g_mComponentContent[KEY_PICTURE]);
}
						
// ------------------------------------------------------------------------------------------------

loadSelection(string shapeName, dyn_string xmlItems)
{
  string sItem;
  bool bSelected;
	shape table = getShape(shapeName);
  
  if(table.lineCount > 0)
  {
		for(int row = 0; row < table.lineCount; row++)
		{
			sItem = table.cellValueRC(row, \"items\");
      bSelected = dynContains(xmlItems, sItem);
      
      table.cellValueRC(row, \"inSelection\", bSelected);
      setRowBackgroundColor(shapeName, row, bSelected);
		}
  }
		
	table.lineVisible(0);
}
							
// ------------------------------------------------------------------------------------------------

dyn_string getSelectedLines(string shapeName)
{
	shape table = getShape(shapeName);
	dyn_string dsItems;
  string sItem;

	if(table.lineCount > 0)
  {
		for(int i = 0; i < table.lineCount; i++)
		{
			sItem = table.cellValueRC(i, \"inSelection\");
			if(sItem == TRUE)
			{
				dynAppend(dsItems, table.cellValueRC(i, \"items\"));
			}  			
		}
  }
		
	return dsItems;
}
						
// ------------------------------------------------------------------------------------------------

loadXMLFromFile(string xmlFileName)
{
  dyn_string  dsConfigFilesGeneral, 
              dsConfigFilesWindows, 
              dsConfigFilesLinux, 
              dsSubComponents, 
              dsScriptsToBeAdded, 
              dsFiles,
              dsOtherFiles;
	bool result = FALSE;
  
  clearComponentContent();

	if(xmlFileName != \"\")
		result = fileToString(xmlFileName, g_sXmlFileContent);
		
	if(result)
	{
		g_bXmlLoaded = TRUE;
		newXmlButton.enabled = TRUE;

		fwInstallationXml_decode(g_sXmlFileContent,
															g_mComponentContent[KEY_NAME], 
                              g_mComponentContent[KEY_VERSION], 
                              g_mComponentContent[KEY_DATE],
															g_mComponentContent[KEY_REQUIRED], 
															g_mComponentContent[KEY_SUBCOMP],
                              g_mComponentContent[KEY_PREINIT],
															g_mComponentContent[KEY_INIT], 
                              g_mComponentContent[KEY_DELETE], 
															g_mComponentContent[KEY_POSTINSTALL], 
                              g_mComponentContent[KEY_POSTDELETE],
															dsConfigFilesGeneral,
															dsConfigFilesWindows,
															dsConfigFilesLinux,
															g_mComponentContent[KEY_DPLIST], 
                              g_mComponentContent[KEY_PANEL],
															g_mComponentContent[KEY_SCRIPT], 
                              g_mComponentContent[KEY_LIBRARY],
															dsOtherFiles, 
                              dsSubComponents,
															dsScriptsToBeAdded, 
                              dsFiles,
                              g_mComponentContent[KEY_RESTART]);

    for(int i = 1; i <= dynlen(dsOtherFiles); i++)
    {
      if(strpos(dsOtherFiles[i], \"bin/\") >= 0)
      {
        dynAppend(g_mComponentContent[KEY_BIN], dsOtherFiles[i]);
      }
      else if(strpos(dsOtherFiles[i], \"help/\") >= 0)
      {
        dynAppend(g_mComponentContent[KEY_HELP], dsOtherFiles[i]);
      }
      else if(strpos(dsOtherFiles[i], \"pictures/\") >= 0)
      {
        dynAppend(g_mComponentContent[KEY_PICTURE], dsOtherFiles[i]);
      }
      else
      {
        dynAppend(g_mComponentContent[KEY_OTHER], dsOtherFiles[i]);
      }
    }

    dynAppend(g_mComponentContent[KEY_CONFIG], dsConfigFilesGeneral);
    dynAppend(g_mComponentContent[KEY_CONFIG], dsConfigFilesWindows);
	  dynAppend(g_mComponentContent[KEY_CONFIG], dsConfigFilesLinux);

		componentName.text = g_mComponentContent[KEY_NAME];

    applyXMLSelection();

		g_dsXmlDescription = strsplit(g_sXmlFileContent, \"\\n\");	
	}

  reloadButton.enabled(FALSE);
}
			
