//===========================================================
  
  system(&quot;mv &quot;+PROJ_PATH+&quot;temp/arduino.png &quot;+dirPathProject+&quot;ArdEnvStation_Plot_&quot; + startString+&quot;_TO_&quot;+endString+&quot;.png&quot;);

  
