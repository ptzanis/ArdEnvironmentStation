  		{
        generationLog.append(\"Destination folder selected:\");
        generationLog.append(destinationFolder);
  			generationLog.append(\"--------------------------------------------------------------------------------------------------\");
        generationLog.append(\"Copying files started.\");

  			destinationFolder += g_sSlash + componentName.text;      
        fwInstallationPackager_packNgo(destinationFolder, sourcePath.text, g_dsXmlDescription);

  			//closeProgressBar();
        generationLog.append(\"Copying files ended.\");
        generationLog.append(\"--------------------------------------------------------------------------------------------------\");
        generationLog.append(\"Component folder successfully generated.\");
        
        showUserFeedback(\"Component folder successfully generated.\", 3);
  		}
      else
      {
        generationLog.append(\"Component folder not generated. No path provided.\");
        showUserFeedback(\"Component folder not generated. No path provided.\", 3);
      }
  	}
    else
    {
      generationLog.append(\"Component folder not generated. No information provided.\");
      showUserFeedback(\"Component folder not generated. No information provided.\", 3);
    }
  }
  else
  {
    generationLog.append(\"Component folder not generated. Name missing.\");
    showUserFeedback(\"Component folder not generated. Name missing.\", 3);
  }

  btnClose.enabled	= true;	 
}	





